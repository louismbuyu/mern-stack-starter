    "@typescript-eslint/parser": "^4.24.0",
    "cors": "^2.8.5",
    "eslint": "^7.27.0",
    "jest": "^26.6.3",
    "mockingoose": "^2.13.0",
    "mongodb-memory-server": "^6.9.6",
