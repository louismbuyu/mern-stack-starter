    "@types/passport-local": "^1.0.33",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "express-winston": "^4.1.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^8.2.2",
    "mongoose": "^5.12.7",
