{
  "name": "starter-pack",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc --build tsconfig3.json",
    "start": "tsc && npm run migrate:up && node dist/app.js",
    "dev": "tsc && npm run migrate:up-dev && nodemon --config nodemon.json src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:up": "migrate-mongo up -f src/config/migration-config.ts",
    "migrate:up-dev": "migrate-mongo up -f src/config/migration-config-local.ts",
    "migrate:down": "migrate-mongo down",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
